<div class="divisiontop light"></div>
<div class="division dark">
    <div class="box light" style="grid-row: 1; grid-column: 1 / 7;">
      <h1 class="box_value"><%- ownRank %> </h1>
		  <h1 class="box_title">Rang</h1>
    </div>
    <div class="box light" style="grid-row: 1; grid-column: 7 / 13;">
      <h1 class="box_value"><%- round %> </h1>
		  <h1 class="box_title">Runde</h1>
    </div>
    <div class="box light" style="grid-row: 2; grid-column: 1 / 13;">
      <table style="grid-row: 2; grid-column: 1 / 13;">
        <% let row = 0 %> 
        <% users.forEach(user => { %>
            <%- include('partials/tournament_table.ejs', {user: user, row: row}) %>
            <% row++ %> 
        <% }) %>
    </table>
    </div>

</div>
<div class="division light">
    <h1 class="division_description" style="grid-column: 1 / 13;">offene Spiele (<%- outstandingMatches.length %>)</h1>
    <% outstandingMatches.forEach(match => { %>
    <%- include('partials/tournament_game_outstanding.ejs', {match: match}) %> 
    <% }) %> 
</div>
<div class="division dark">
    <h1 class="division_description" style="grid-column: 1 / 13;">gespielte Spiele (<%- finishedMatches.length %>)</h1>
    <% finishedMatches.forEach(match => { %>
        <%- include('partials/tournament_game_finished.ejs', {match: match}) %> 
    <% }) %> 
</div>