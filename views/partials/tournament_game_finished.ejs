<div class="rowContainer" style="grid-column: 1 / 13;">
    <label class="group"><%- match.group %></label>
    <label class="date"><%- match.finishDate %> </label>
    <div class="playerImg imgLeft" style="grid-column: 1; grid-row: 2"></div>
    <label class="player" style="grid-column: 2; grid-row: 2"><%- match.playerA1 %></label>
    <label class="score" style="grid-column: 3; grid-row: 2"><%- match.scoreA %></label>
    <label class="score" style="grid-column: 4; grid-row: 2"><%- match.scoreB %></label>
    <label class="player" style="grid-column: 5; grid-row: 2"><%- match.playerB1 %></label>
    <div class="playerImg imgRight" style="grid-column: 6; grid-row: 2"></div>
  </div>